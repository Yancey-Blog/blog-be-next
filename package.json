{
  "name": "blog-be-next",
  "version": "2.0.0",
  "description": "The backend platform for Yancey blog.",
  "repository": "git@github.com:Yancey-Blog/blog-be-next.git",
  "author": "YanceyOfficial <yanceyofficial@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "build": "NODE_ENV=production yarn clear && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch": "NODE_ENV=development nest start --watch",
    "commit": "npx git-cz",
    "push": "git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && yarn push",
    "release:minor": "standard-version --release-as minor && yarn push",
    "release:patch": "standard-version --release-as patch && yarn push",
    "document": "npx compodoc -p tsconfig.json -s",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codecov": "node_modules/.bin/codecov -t 7b33b616-c7c6-465f-a858-91ca93071f98"
  },
  "dependencies": {
    "@apollo/gateway": "^0.43.1",
    "@azure/storage-blob": "^12.8.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^9.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.7.2",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "luxon": "^2.0.2",
    "mongoose": "^6.0.6",
    "morgan": "^1.10.0",
    "nest-winston": "^1.6.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rxjs": "^7.3.0",
    "serve-favicon": "^2.5.0",
    "speakeasy": "^2.0.0",
    "ts-morph": "^12.2.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yancey-js-util": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/csurf": "^1.11.2",
    "@types/jest": "^27.0.1",
    "@types/luxon": "^2.0.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/qrcode": "^1.4.1",
    "@types/ramda": "^0.27.54",
    "@types/request-ip": "^0.0.37",
    "@types/serve-favicon": "^2.5.3",
    "@types/speakeasy": "^2.0.6",
    "@types/supertest": "^2.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "codecov": "^3.8.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-nestjs": "^1.2.3",
    "husky": "^4.3.8",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
