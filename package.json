{
  "name": "blog-be-next",
  "version": "0.15.0",
  "description": "The backend platform for Yancey blog.",
  "repository": "git@github.com:Yancey-Blog/blog-be-next.git",
  "author": "YanceyOfficial <yanceyofficial@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "build": "yarn clear && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "deploy": "node_modules/.bin/pm2-runtime dist/main.js",
    "watch": "NODE_ENV=development nest start --watch",
    "commit": "npx git-cz",
    "push": "git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && yarn push",
    "release:minor": "standard-version --release-as minor && yarn push",
    "release:patch": "standard-version --release-as patch && yarn push",
    "document": "npx compodoc -p tsconfig.json -s --language \"ja-JP\"",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codecov": "node_modules/.bin/codecov -t 7b33b616-c7c6-465f-a858-91ca93071f98"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.1.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.2.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "ali-oss": "^6.4.0",
    "apollo-server-express": "^2.11.0",
    "axios": "^0.19.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "helmet": "^3.21.3",
    "moment": "^2.24.0",
    "mongodb": "^3.5.2",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "serve-favicon": "^2.5.0",
    "uuid": "^7.0.2",
    "yancey-js-util": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/cli": "^7.1.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/ali-oss": "^6.0.4",
    "@types/bcrypt": "^3.0.0",
    "@types/csurf": "^1.9.36",
    "@types/express-rate-limit": "^5.0.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.4",
    "@types/mongoose": "^5.7.6",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.9.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/ramda": "^0.26.44",
    "@types/serve-favicon": "^2.5.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "codecov": "^3.6.5",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-nestjs": "^1.2.3",
    "husky": "^4.0.10",
    "jest": "^25.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "standard-version": "^7.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js,scss,css}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
