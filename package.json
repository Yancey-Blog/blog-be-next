{
  "name": "blog-be-next",
  "version": "0.6.0",
  "description": "The backend platform for Yancey blog.",
  "private": true,
  "repository": "git@github.com:Yancey-Blog/blog-be-next.git",
  "author": "YanceyOfficial <yanceyofficial@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "build": "yarn clear && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "watch": "NODE_ENV=development nest start --watch",
    "commit": "npx git-cz",
    "push": "git push --follow-tags origin master",
    "release:major": "node_modules/standard-version/bin/cli.js --release-as major && yarn push",
    "release:minor": "node_modules/standard-version/bin/cli.js --release-as minor && yarn push",
    "release:patch": "node_modules/standard-version/bin/cli.js --release-as patch && yarn push",
    "document": "npx compodoc -p tsconfig.json -s --language \"ja-JP\"",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codecov": "node_modules/codecov/bin/codecov -t 7b33b616-c7c6-465f-a858-91ca93071f98"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@hapi/joi": "^16.1.8",
    "@nestjs/common": "^6.10.8",
    "@nestjs/core": "^6.10.8",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-fastify": "^6.10.12",
    "ali-oss": "^6.3.1",
    "apollo-server-fastify": "^2.9.14",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "fastify-cors": "^3.0.0",
    "fastify-favicon": "^2.0.0",
    "fastify-healthcheck": "^2.1.0",
    "fastify-helmet": "^3.0.2",
    "fastify-rate-limit": "^3.0.1",
    "graphql": "^14.5.8",
    "moment": "^2.24.0",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "type-graphql": "^0.17.6",
    "uuid": "^3.3.3",
    "yancey-js-util": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.7.5",
    "@nestjs/testing": "^6.10.11",
    "@types/ali-oss": "^6.0.4",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/fastify-favicon": "^0.3.0",
    "@types/hapi__joi": "^16.0.4",
    "@types/jest": "^24.0.23",
    "@types/mongoose": "^5.5.34",
    "@types/node": "^12.12.21",
    "@types/passport-jwt": "^3.0.3",
    "@types/ramda": "^0.26.38",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.12.0",
    "codecov": "^3.6.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js,scss,css}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
